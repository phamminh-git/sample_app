<% provide :title, t(".page_title") %>

<table class="table">
  <thead>
    <tr>
      <th scope="col"><%= t ".thead_name" %></th>
      <th scope="col"><%= t ".thead_email" %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
